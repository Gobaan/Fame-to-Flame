class FeedEntry:
    """
    Represents an entry in a RSS or ATOM feed.
    """

    def __init__(self,
                 guid,
                 url,
                 content,
                 rawcontent,
                 title,
                 link,
                 author,
                 comments,
                 updated):
        """
        """
        self.guid = guid;
        self.feedurl = url;
        self.content = content;
        self.rawcontent = content;
        self.title = title;
        self.link = link;
        self.author = author;
        self.comments = comments;
        self.published = updated;
